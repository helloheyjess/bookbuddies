<%= form_for(@group) do |f| %>

	<%= f.label :group_name %>
	<%= f.text_field :group_name %>

	<%= f.label :book_name %>
	<%= f.text_field :book_name %>

	<%= f.label :author %>
	<%= f.text_field :author %>

	<%= f.label :description %>
	<%= f.text_area :description %>

	<%= f.label :location %>
	<%= f.text_field :location %>

	<%= f.submit %>
<% end %>